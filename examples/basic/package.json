{
  "name": "basic-example",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && npm run build:server && npm run build:client",
    "build:client": "cross-env NODE_ENV=production vite build",
    "build:server": "cross-env NODE_ENV=production vite build --ssr src/index.ts",
    "dev": "cross-env NODE_ENV=development tsx --enable-source-maps src/index.ts",
    "start": "cross-env NODE_ENV=production node --enable-source-maps dist/index.js"
  },
  "devDependencies": {
    "@marko/compiler": "^5.22.6",
    "@marko/tags-api-preview": "^0.5.6",
    "@marko/vite": "^2.3.7",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.6",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "marko": "^5.21.9",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tsx": "^3.8.2",
    "typescript": "^4.7.4",
    "undici": "^5.10.0",
    "vite": "^3.0.8"
  }
}
